<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>

<!-- footer -->
<div class="container-fluid footer py-4">
    <div class="container">
        <div class="row g-4 align-items-center">
            <div class="col-md-6 text-center text-md-start mb-md-0">
                <span class="text-white"><i class="fas fa-copyright text-light me-2"></i>Hobby Mate, All rights reserved.</span>
            </div>
            <div class="col-md-6 text-center text-md-end text-white">
                Designed By HanJunHee
            </div>
        </div>
    </div>
</div>
<!-- footer -->

<!-- JavaScript Libraries -->
<!-- jQuery 라이브러리: DOM 조작, 이벤트 처리 등을 간편하게 하기 위해 사용 -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
<!-- Bootstrap JavaScript: 부트스트랩의 컴포넌트 (모달, 캐러셀 등)를 사용하기 위해 필요 -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
<!-- WOW.js: 스크롤 시 애니메이션 효과를 제공하는 라이브러리 -->
<script src="${pageContext.request.contextPath}/js/wow.min.js"></script>
<!-- Easing.js: 애니메이션의 가속/감속 효과를 추가하여 더 부드럽게 만들기 위한 라이브러리 -->
<script src="${pageContext.request.contextPath}/js/easing.min.js"></script>
<!-- Waypoints.js: 특정 요소가 화면에 보일 때 이벤트를 트리거할 수 있도록 하는 라이브러리 -->
<script src="${pageContext.request.contextPath}/js/waypoints.min.js"></script>
<!-- Counter Up: 숫자 증가 애니메이션을 제공하는 라이브러리 -->
<script src="${pageContext.request.contextPath}/js/counterup.min.js"></script>
<!-- Owl Carousel: 슬라이드 형태의 캐러셀을 구현하는 데 사용되는 라이브러리 -->
<script src="${pageContext.request.contextPath}/js/owl.carousel.min.js"></script>
<!-- Template Javascript: 웹사이트의 사용자 정의 스크립트를 포함 -->
<script src="${pageContext.request.contextPath}/js/main.js"></script>
<!-- 날씨 api 관련 커스텀 js -->
<script src="${pageContext.request.contextPath}/js/weather.js"></script>